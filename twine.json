{
    "title": "OpenFAST twine",
    "description": "Uses BEM theory to model wind turbines",
    "configuration_values_schema": {
        "type": "object",
        "properties": {
            "turbine_model":{
                "title": "Wind Turbine Model",
                "type": "string"
            }
        }
    },
    "input_values_schema": {
        "title": "Input values schema",
        "type": "object",
        "properties": {
            "model_case":{
                "title": "Model Case",
                "description": "OpenFAST input file *.fst",
                "type": "string"
            },
            "u_ref": {
                "title": "U",
                "description": "Mean (total) velocity at the reference height [m/s]",
                "type": "number",
                "default": 10,
                "minimum": 1e-6,
                "maximum": 330
            }
        }
    }
}
